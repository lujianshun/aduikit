<resources>
    <!-- Default screen margins, per the Android Design guidelines. -->
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <!--字体大小-->
    <!--大标题,如导航栏标题,大按钮字体等-->
    <dimen name="text_largest">18sp</dimen>
    <!--单行列表内,主题文字-->
    <dimen name="text_large">16sp</dimen>
    <!--此字号服务主标题,并与之关联-->
    <dimen name="text_mid">15sp</dimen>
    <!--正文文字,页面辅助文字等-->
    <dimen name="text_small">14sp</dimen>
    <!--协议提示-->
    <dimen name="text_xieyi">13sp</dimen>
    <!--头部提示-->
    <dimen name="text_smaller">13sp</dimen>
    <!--最小说明文体,提示性文字例如版权信息等用户不需要特别关注的信息-->
    <dimen name="text_smallest">12sp</dimen>
    <!--分割线-->
    <dimen name="divide_height">0.5dp</dimen>
</resources>
